<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Pacs</Name>
<Description>PACS server instance</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/pacs/</Registry>
<Repository>sparklyballs/pacs</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8080</HostPort>
<ContainerPort>8080</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>11112</HostPort>
<ContainerPort>11112</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8080]/dcm4chee-web3</WebUI>
<Banner></Banner>
<Icon>http://www.blingblog.info/wp-content/uploads/2006/11/5908-pcrwr-grabit.png</Icon>
</Container>
