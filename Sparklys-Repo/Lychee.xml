<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Lychee</Name>
<Description>Lychee is a free photo-management tool, which runs on your server or web-space.[br]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/lychee/</Registry>
<Repository>sparklyballs/lychee</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/Lychee</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner></Banner>
<Icon>http://img4.wikia.nocookie.net/__cb20140620094320/plantsvszombies/images/2/2f/Lychee.png</Icon>
</Container>
