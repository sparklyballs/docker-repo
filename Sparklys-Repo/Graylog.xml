<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Graylog</Name>
<Beta>False</Beta>
<Category>Tools:</Category>
<Overview>Graylog log analyzer, template for easier implementation to unraid.</Overview>
<Support>http://lime-technology.com/forum/index.php?topic=37209</Support>
<Description>Graylog log analyzer</Description>
<Registry>https://registry.hub.docker.com/u/graylog2/allinone/</Registry>
<Repository>graylog2/allinone</Repository>
<BindTime>true</BindTime>
<Privileged>true</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>9000</HostPort>
<ContainerPort>9000</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>12201</HostPort>
<ContainerPort>12201</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>514</HostPort>
<ContainerPort>514</ContainerPort>
<Protocol>udp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name>GRAYLOG_PASSWORD</Name>
<Value></Value>
</Variable>
<Variable>
<Name>GRAYLOG_TIMEZONE</Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/opt/graylog/data</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/log/graylog</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/opt/graylog/plugin</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:9000]/</WebUI>
<Banner>http://i.imgur.com/G8zlNUY.png</Banner>
<Icon>http://i.imgur.com/O4mraK7.png</Icon>
</Container>
